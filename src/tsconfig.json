{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "jsx": "preserve",
    "target": "es5",
    "allowJs": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "exclude": ["node_modules"],
  "include": ["**/*.ts", "**/*.tsx"]
}

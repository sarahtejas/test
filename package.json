{
  "name": "web-ui-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "chromatic": "CHROMATIC_APP_CODE=$CHROMATIC_APP_CODE chromatic test --storybook-build-dir /home/circleci/web-ui/storybook-static --exit-zero-on-changes",
    "lint": "eslint . --ext .ts,.tsx",
    "prettier": "prettier ./src/**/*.{ts,tsx} --write",
    "show-coverage": "open coverage/lcov-report/index.html &>/dev/null || xdg-open coverage/lcov-report/index.html &>/dev/null",
    "start": "next dev",
    "start-production": "NODE_ENV=production next start -p ${PORT:-80}",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "jest --runInBand",
    "test:ci": "yarn test --coverage --reporters=default --reporters=jest-junit",
    "test:coverage": "yarn test --coverage && yarn show-coverage",
    "test:dev": "yarn test --watch",
    "test:e2e:run": "yarn wait-on 'http://localhost:3000/' && yarn cypress run",
    "test:e2e:dev": "yarn wait-on 'http://localhost:3000/' && yarn cypress open",
    "typecheck": "tsc --noEmit"
  },
  "resolutions": {
    "axios": "0.21.1",
    "node-fetch": "2.6.1",
    "lodash": "4.17.20",
    "markdown-to-jsx": "7.1.0",
    "minimist": "1.2.5",
    "serialize-javascript": "5.0.1",
    "ini": "1.3.6",
    "yargs-parser": "20.2.4",
    "tree-kill": "1.2.2"
  },
  "resolutions:comments": {
    "markdown-to-jsx": "2020-05-20: All versions of markdown-to-jsx have a vulnerability, but are only used by Storybook. This uses a version that's marked as safe, but may not be compatible. We don't think we ever use it, but if we do, Storybook may break, but not the app. https://www.npmjs.com/advisories/1219",
    "node-fetch": "2020-10-19: cross-fetch@3.0.4 has a pinned dependency on node-fetch@2.6.0, which has a vulnerability patched in 2.6.1."
  },
  "dependencies": {
    "@circleci/analytics": "0.8.1",
    "@circleci/compass": "4.36.3",
    "@circleci/exception-logger": "0.4.0",
    "@circleci/theme": "7.0.0",
    "@emotion/core": "10.1.1",
    "@emotion/styled": "10.0.27",
    "@zeit/next-css": "1.0.1",
    "@zeit/next-source-maps": "0.0.4-canary.1",
    "emotion": "10.0.27",
    "emotion-normalize": "10.1.0",
    "emotion-server": "10.0.27",
    "emotion-theming": "10.0.27",
    "fork-ts-checker-webpack-plugin": "3.0.1",
    "next": "10.0.7",
    "react": "16.14.0",
    "react-dom": "16.14.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@storybook/addon-a11y": "6.1.15",
    "@storybook/addon-actions": "6.1.15",
    "@storybook/addon-docs": "^6.1.15",
    "@storybook/addon-knobs": "6.1.15",
    "@storybook/addon-storyshots": "6.1.15",
    "@storybook/addon-viewport": "6.1.15",
    "@storybook/react": "6.1.15",
    "@storybook/theming": "6.1.15",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "10.4.9",
    "@testing-library/react-hooks": "3.4.2",
    "@types/cypress-axe": "0.8.0",
    "@types/jest": "26.0.20",
    "@types/next": "8.0.6",
    "@types/react": "16.9.56",
    "@types/storybook__addon-actions": "3.4.3",
    "@types/storybook__react": "4.0.2",
    "@typescript-eslint/eslint-plugin": "4.15.0",
    "@typescript-eslint/eslint-plugin-tslint": "4.15.0",
    "@typescript-eslint/parser": "4.15.0",
    "babel-loader": "8.2.2",
    "chromatic": "5.4.0",
    "codecov": "3.8.1",
    "cypress": "4.12.1",
    "cypress-axe": "0.10.0",
    "dotenv": "8.2.0",
    "eslint": "7.13.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "23.20.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "fetch-mock": "9.10.7",
    "jest": "25.5.4",
    "jest-date-mock": "1.0.8",
    "jest-junit": "11.1.0",
    "jest-raw-loader": "1.0.1",
    "prettier": "2.1.2",
    "react-test-renderer": "16.14.0",
